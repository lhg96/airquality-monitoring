# 환경 변수 설정 파일
# 이 파일은 Git에 커밋하지 마세요!

# 에어코리아 API 키
AIRKOREA_API_KEY=your-api-key-here

# 관리자 계정
ADMIN_USER=admin
ADMIN_PASSWORD=your-secure-password-here

# 데이터베이스 설정
DB_HOST=localhost
DB_PORT=5432
DB_NAME=airquality
DB_USER=postgres
DB_PASSWORD=your-db-password-here

# Google App Engine 설정
GAE_PROJECT_ID=your-gae-project-id
GAE_SERVICE_ACCOUNT_KEY=path/to/service-account-key.json

# 타이머 설정 (초)
DATA_COLLECTION_INTERVAL=3600

# 센서 보정 임계값
CALIBRATION_UPPER_THRESHOLD=100
CALIBRATION_LOWER_THRESHOLD=0
